{
  "name": "@sanofi-iadc/glisten",
  "version": "0.0.28",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name @sanofi-iadc/glisten ./src/exports.ts",
    "prepare": "npm run build-bundle",
    "start": "npm run serve",
    "test": "npm run test:e2e && npm run test:unit:cov",
    "test:unit:cov": "vue-cli-service test:unit --coverage",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./deploy-docs.sh"
  },
  "main": "dist/@sanofi-iadc/glisten.umd.js",
  "typings": "types.d.ts",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.d.ts"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.165",
    "apexcharts": "^3.22.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.2",
    "babel-polyfill": "^6.26.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.0",
    "subscriptions-transport-ws": "^0.9.16",
    "vue": "^2.6.12",
    "vue-apexcharts": "^1.6.0",
    "vue-apollo": "^3.0.0",
